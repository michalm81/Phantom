<Window x:Class="Magazyn_App.View.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:local="clr-namespace:Magazyn_App"
        xmlns:views="clr-namespace:Magazyn_App.View"
        xmlns:vm="clr-namespace:Magazyn_App.ViewModel"
        Title="Historia Dostaw" Height="320" Width="600">
    <i:Interaction.Behaviors>
        <local:WindowCloseBehavior x:Name="buttonclose" ButtonControl="{Binding ElementName=closingButton}"/>
    </i:Interaction.Behaviors>
    <Grid>
        <views:UserControl4Dostawy/>
        <Button Style="{StaticResource storebuttonstyle}" x:Name="closingButton" Content="OK" HorizontalAlignment="Left" Margin="101,250,0,0" VerticalAlignment="Top" Width="397" Height="20"/>
    </Grid>
</Window>
