<Window x:Class="Magazyn_App.View.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:local="clr-namespace:Magazyn_App"
        xmlns:views="clr-namespace:Magazyn_App.View"
        xmlns:vm="clr-namespace:Magazyn_App.ViewModel"
        Title="Dostawa" Height="310" Width="550" ResizeMode="NoResize">

    <i:Interaction.Behaviors>
        <local:WindowCloseBehavior x:Name="buttonclose" ButtonControl="{Binding ElementName=closingButton}"/>
    </i:Interaction.Behaviors>
    <Grid>
        <views:UserControl3 />
        <Button Style="{StaticResource storebuttonstyle}" x:Name="closingButton" Content="OK" HorizontalAlignment="Left" Margin="421,238.5,0,0" VerticalAlignment="Top" Width="90" Height="20"/>

    </Grid>
</Window>
